}

/* Exit screen */
exit_page {
    align: center middle;
}

#exit_page {
    grid-size: 2;
    grid-gutter: 1 2;
    grid-rows: 1fr 3;
    padding: 0 1;
    width: 60;
    height: 11;
    border: thick $background 80%;
    background: $surface;
}

#exit_question {
    column-span: 2;
    height: 1fr;
    width: 1fr;
    content-align: center middle;
}

#exit_button, #cancel_exit_button {
    width: 100%;
}

