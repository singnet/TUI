/* Welcome screen */
#welcome_screen {
    align: center middle;
    background: #f6d7b0;
    width: 100w;
    height: 100h;
}

#welcome_screen_content {
    align: center middle;
    background: #2d2d2d;
    height: 50h;
    width: 90w;
}

#welcome_label_1 {
    color: #f6d7b0;
    margin-left: 21;
}

#welcome_label_2 {
    color: #f6d7b0;
    margin-left: 7;
}

#start_button {
    background: #0185b5;
    color: #f6d7b0;
    border: none;
    height: 3;
    width: 8;
    margin-left: 20;
}

#start_button:focus {
    text-style: none;
}

#start_button:hover {
    background: #05b9fa;
}

/* Error Exit screen */
#error_exit_log {
    align: center middle;
    width: 100%;
    height: 80%;
    margin-left: 5;
    margin-right: 5;
    margin-top: 5;
    border: solid white
}

#error_exit_button {
    align: center middle;
    width: 40%;
    border: solid white;
    background: rgba(40, 40, 40, 0.5);
    margin-left: 55;
    margin-top: 5;
}

/* Popup screen */
#popup_output_log {
    align: center middle;
    width: 100%;
    height: 80%;
    margin-left: 5;
    margin-right: 5;
    margin-top: 5;
    border: solid white
}

#output_exit_button {
    align: center middle;
    width: 40%;
    border: solid white;
    background: rgba(40, 40, 40, 0.5);
    margin-left: 55;
    margin-top: 5;
}

/* Conditional screen */
#conditional_input_log {
    align: center middle;
    width: 100%;
    height: 80%;
    margin-left: 5;
    margin-right: 5;
    margin-top: 5;
    border: solid white
}

#conditional_input_accept_button {
